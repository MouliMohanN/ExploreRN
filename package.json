{
  "name": "ExploreRN",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ts:check": "tsc -p . --noEmit",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "build:release:android": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release -Dorg.gradle.jvmargs=-Xmx4g",
    "build:release:ios": "cd ios && pod install && cd .. && xcodebuild ONLY_ACTIVE_ARCH=YES -workspace ios/AwesomeProject.xcworkspace -UseNewBuildSystem=YES -scheme AwesomeProject -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
    "detox:test:android": "detox test --config-path .detoxrc-ci.js --configuration android.emu.release --cleanup",
    "detox:test:ios": "detox test --config-path .detoxrc-ci.js --configuration ios.sim.release --cleanup",
    "fingerprint:android": "npx expo-updates fingerprint:generate --platform android | jq -r '.hash' | xargs -n 1 echo 'fingerprint:'",
    "fingerprint:ios": "npx expo-updates fingerprint:generate --platform ios | jq -r '.hash' | xargs -n 1 echo 'fingerprint:'",
    "build:debug:android": "cd android && ./gradlew assembleDebug -DtestBuildType=debug -Dorg.gradle.jvmargs=-Xmx4g",
    "build:debug:ios": "cd ios && pod install && cd .. && xcodebuild ONLY_ACTIVE_ARCH=YES -workspace ios/AwesomeProject.xcworkspace -UseNewBuildSystem=YES -scheme AwesomeProject -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -quiet",
    "maestro:test": "maestro test --debug-output maestro-debug-output .maestro",
    "adb-install": "adb install android/app/build/outputs/apk/debug/app-debug.apk && adb reverse tcp:8081 tcp:8081 && npm run start",
    "adb-open-app": "adb shell am start -n com.awesomeproject/com.awesomeproject.MainApplication",
    "adb-close-app": "adb shell am force-stop com.awesomeproject",
    "adb-shell-top": "adb shell top -m 50",
    "adb-shell-recents": "adb shell dumpsys activity recents",
    "adb-shell-remove-stack": "adb shell am stack remove 170",
    "adb-connect": "adb reverse tcp:8081 tcp:8081",
    "adb-connect-dev-tools": "adb reverse tcp:8097 tcp:8097",
    "adb-uninstall": "adb uninstall com.awesomeproject",
    "clean-install-android": "remove-node-modules && npm install -f && npm run android",
    "clean-install-android-release": "remove-node-modules && npm install -f && cd android && gradlew installRelease && cd ..",
    "generate-spec": "cd android && gradlew generateCodegenArtifactsFromSchema && cd ..",
    "rn-info": "npx react-native-info"
  },
  "dependencies": {
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "react": "19.0.0",
    "react-native": "0.79.1",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-gesture-handler": "2.25.0",
    "react-native-paper": "5.13.3",
    "react-native-reanimated": "3.17.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.1",
    "@react-native/eslint-config": "0.79.1",
    "@react-native/metro-config": "0.79.1",
    "@react-native/typescript-config": "0.79.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "3.5.3",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4",
    "typescript-eslint": "8.31.0"
  },
  "engines": {
    "node": ">=18"
  }
}
